{
  "name": "@alcom/api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "NODE_ENV=production tsx prisma/seed-production.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@alcom/shared": "workspace:*",
    "@prisma/client": "^6.3.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "resend": "^4.1.2",
    "uuid": "^11.0.0",
    "zod": "^3.23.0",
    "tsx": "^4.19.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.3.0",
    "supertest": "^7.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  }
